<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".report3">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="71dp"
        android:clickable="true"
        android:drawableLeft="@drawable/battery"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/dzplot"
        android:layout_width="401dp"
        android:layout_height="119dp"
        android:layout_below="@+id/textView17"
        android:layout_marginTop="1dp"
        android:clickable="true" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/dxyPlot"
        android:layout_width="401dp"
        android:layout_height="145dp"
        android:layout_below="@+id/dzplot"
        android:layout_marginTop="4dp"
        android:clickable="true" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/rvPlot"
        android:layout_width="401dp"
        android:layout_height="128dp"
        android:layout_below="@+id/aPlot"
        android:layout_marginTop="4dp"
        android:clickable="true" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/aPlot"
        android:layout_width="401dp"
        android:layout_height="138dp"
        android:layout_below="@+id/dxyPlot"
        android:layout_marginTop="7dp"
        android:clickable="true" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="match_parent"
        android:layout_height="79dp"
        android:layout_below="@+id/rvPlot"
        android:layout_marginTop="14dp"
        android:clickable="true"
        android:gravity="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

</RelativeLayout>